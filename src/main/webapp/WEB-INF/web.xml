<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://java.sun.com/xml/ns/javaee" 
    xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
    id="WebApp_ID" version="2.5">
     
    <display-name>SimpleServlet Web Application</display-name>
       <servlet>
        <servlet-name>SimpleServlet</servlet-name>
        <servlet-class>org.dstech.servlet.SimpleServlet</servlet-class>
    </servlet>
 
    <servlet-mapping>
        <servlet-name>SimpleServlet</servlet-name>
        <url-pattern>/SimpleServlet</url-pattern>
    </servlet-mapping>

 <servlet>
        <servlet-name>ServeltRequest</servlet-name>
        <servlet-class>org.dstech.servlet.ServeltRequest</servlet-class>
    </servlet>
 
    <servlet-mapping>
        <servlet-name>ServeltRequest</servlet-name>
        <url-pattern>/ServeltRequest</url-pattern>
    </servlet-mapping>

 <servlet>
        <servlet-name>Test</servlet-name>
        <servlet-class>org.dstech.servlet.test</servlet-class>
    </servlet>
 
    <servlet-mapping>
        <servlet-name>Test</servlet-name>
        <url-pattern>/Test</url-pattern>
    </servlet-mapping>


    </web-app>